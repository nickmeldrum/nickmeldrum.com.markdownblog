@model MarkdownBlog.Net.Web.Models.Post
@{
    ViewBag.Title = Model.Metadata.Title;
    ViewBag.Jumbotron = PostHelpers.PostJumbotron(Model.Metadata);
}

<article>
    @Html.Raw(Model.Body)
</article>

@Html.Partial("Disqus", Model.Disqus)

@section asides
{
    @Html.Partial("Asides", Model)
}
